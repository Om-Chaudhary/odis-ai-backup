{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs",
  "tags": ["type:app", "scope:docs", "platform:web"],
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus start",
        "cwd": "apps/docs"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus build",
        "cwd": "apps/docs"
      },
      "cache": true,
      "outputs": ["{projectRoot}/build"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus serve",
        "cwd": "apps/docs"
      },
      "dependsOn": ["build"]
    },
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus clear",
        "cwd": "apps/docs"
      }
    },
    "swizzle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus swizzle",
        "cwd": "apps/docs"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus deploy",
        "cwd": "apps/docs"
      },
      "dependsOn": ["build"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/docs"
      },
      "cache": true
    },
    "write-translations": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus write-translations",
        "cwd": "apps/docs"
      }
    },
    "write-heading-ids": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docusaurus write-heading-ids",
        "cwd": "apps/docs"
      }
    }
  }
}
