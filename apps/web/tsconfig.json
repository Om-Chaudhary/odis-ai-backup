{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "plugins": [{ "name": "next" }],
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "~/database.types": ["../../libs/types/src/database.types.ts"],
      "@odis/types": ["../../libs/types/src/index.ts"],
      "@odis/types/*": ["../../libs/types/src/*"],
      "@odis/validators": ["../../libs/validators/src/index.ts"],
      "@odis/validators/*": ["../../libs/validators/src/*"],
      "@odis/services": ["../../libs/services/src/index.ts"],
      "@odis/services/*": ["../../libs/services/src/*"],
      "@odis/utils": ["../../libs/utils/src/index.ts"],
      "@odis/utils/*": ["../../libs/utils/src/*"],
      "@odis/db": ["../../libs/db/src/index.ts"],
      "@odis/db/*": ["../../libs/db/src/*"],
      "@odis/ui": ["../../libs/ui/src/index.ts"],
      "@odis/ui/*": ["../../libs/ui/src/*"],
      "@odis/vapi": ["../../libs/vapi/src/index.ts"],
      "@odis/vapi/*": ["../../libs/vapi/src/*"],
      "@odis/idexx": ["../../libs/idexx/src/index.ts"],
      "@odis/idexx/*": ["../../libs/idexx/src/*"],
      "@odis/api-client": ["../../libs/api-client/src/index.ts"],
      "@odis/api-client/*": ["../../libs/api-client/src/*"],
      "@odis/email": ["../../libs/email/src/index.ts"],
      "@odis/email/*": ["../../libs/email/src/*"],
      "@odis/ai": ["../../libs/ai/src/index.ts"],
      "@odis/ai/*": ["../../libs/ai/src/*"],
      "@odis/logger": ["../../libs/logger/src/index.ts"],
      "@odis/logger/*": ["../../libs/logger/src/*"],
      "@odis/api": ["../../libs/api/src/index.ts"],
      "@odis/api/*": ["../../libs/api/src/*"],
      "@odis/clinics": ["../../libs/clinics/src/index.ts"],
      "@odis/clinics/*": ["../../libs/clinics/src/*"],
      "@odis/qstash": ["../../libs/qstash/src/index.ts"],
      "@odis/qstash/*": ["../../libs/qstash/src/*"],
      "@odis/crypto": ["../../libs/crypto/src/index.ts"],
      "@odis/crypto/*": ["../../libs/crypto/src/*"],
      "@odis/resend": ["../../libs/resend/src/index.ts"],
      "@odis/resend/*": ["../../libs/resend/src/*"],
      "@odis/retell": ["../../libs/retell/src/index.ts"],
      "@odis/retell/*": ["../../libs/retell/src/*"],
      "@odis/constants": ["../../libs/constants/src/index.ts"],
      "@odis/constants/*": ["../../libs/constants/src/*"],
      "@odis/env": ["../../libs/env/src/index.ts"],
      "@odis/env/*": ["../../libs/env/src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.cjs",
    "**/*.js",
    ".next/types/**/*.ts",
    "../../libs/**/*.ts",
    "../../libs/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/__tests__/**",
    "src/test/**/*",
    "vitest.config.ts",
    "../../libs/testing/src/**/*"
  ]
}
