{
  "expo": {
    "name": "OdisAI",
    "slug": "odisai-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "odisai",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d3d38"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.odisai.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0d3d38"
      },
      "package": "com.odisai.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-web-browser",
        {
          "experimentalLaunchChromeNativeWindow": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
