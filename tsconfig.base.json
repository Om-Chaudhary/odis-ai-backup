{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "bundler",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "module": "ESNext",
    "lib": ["es2022", "dom", "dom.iterable"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@odis/types": ["libs/types/src/index.ts"],
      "@odis/types/*": ["libs/types/src/*"],
      "@odis/validators": ["libs/validators/src/index.ts"],
      "@odis/validators/*": ["libs/validators/src/*"],
      "@odis/services": ["libs/services/src/index.ts"],
      "@odis/services/*": ["libs/services/src/*"],
      "@odis/utils": ["libs/utils/src/index.ts"],
      "@odis/utils/*": ["libs/utils/src/*"],
      "@odis/db": ["libs/db/src/index.ts"],
      "@odis/db/*": ["libs/db/src/*"],
      "@odis/ui": ["libs/ui/src/index.ts"],
      "@odis/ui/*": ["libs/ui/src/*"],
      "@odis/vapi": ["libs/vapi/src/index.ts"],
      "@odis/vapi/*": ["libs/vapi/src/*"],
      "@odis/idexx": ["libs/idexx/src/index.ts"],
      "@odis/idexx/*": ["libs/idexx/src/*"],
      "@odis/api-client": ["libs/api-client/src/index.ts"],
      "@odis/api-client/*": ["libs/api-client/src/*"],
      "@odis/email": ["libs/email/src/index.ts"],
      "@odis/email/*": ["libs/email/src/*"],
      "@odis/ai": ["libs/ai/src/index.ts"],
      "@odis/ai/*": ["libs/ai/src/*"],
      "@odis/logger": ["libs/logger/src/index.ts"],
      "@odis/logger/*": ["libs/logger/src/*"],
      "@odis/api": ["libs/api/src/index.ts"],
      "@odis/api/*": ["libs/api/src/*"],
      "@odis/clinics": ["libs/clinics/src/index.ts"],
      "@odis/clinics/*": ["libs/clinics/src/*"],
      "@odis/qstash": ["libs/qstash/src/index.ts"],
      "@odis/qstash/*": ["libs/qstash/src/*"],
      "@odis/crypto": ["libs/crypto/src/index.ts"],
      "@odis/crypto/*": ["libs/crypto/src/*"],
      "@odis/resend": ["libs/resend/src/index.ts"],
      "@odis/resend/*": ["libs/resend/src/*"],
      "@odis/retell": ["libs/retell/src/index.ts"],
      "@odis/retell/*": ["libs/retell/src/*"],
      "@odis/constants": ["libs/constants/src/index.ts"],
      "@odis/constants/*": ["libs/constants/src/*"],
      "@odis/testing": ["libs/testing/src/index.ts"],
      "@odis/testing/*": ["libs/testing/src/*"],
      "@odis/env": ["libs/env/src/index.ts"],
      "@odis/env/*": ["libs/env/src/*"],
      "~/lib/utils/*": ["libs/utils/src/*"],
      "~/lib/services/*": ["libs/services/src/*"],
      "~/lib/validators/*": ["libs/validators/src/*"],
      "~/lib/db/*": ["libs/db/src/*"],
      "~/lib/ui/*": ["libs/ui/src/*"],
      "~/lib/vapi/*": ["libs/vapi/src/*"],
      "~/lib/idexx/*": ["libs/idexx/src/*"],
      "~/lib/api-client/*": ["libs/api-client/src/*"],
      "~/lib/clinics/*": ["libs/clinics/src/*"],
      "~/lib/ai/*": ["libs/ai/src/*"],
      "~/lib/qstash/*": ["libs/qstash/src/*"],
      "~/lib/resend/*": ["libs/resend/src/*"],
      "~/lib/email": ["libs/email/src/index.ts"],
      "~/lib/email/*": ["libs/email/src/*"],
      "~/lib/logger": ["libs/logger/src/index.ts"],
      "~/lib/api/*": ["libs/api/src/*"],
      "~/lib/constants/*": ["libs/constants/src/*"],
      "~/lib/llamaindex/*": ["apps/web/src/lib/llamaindex/*"],
      "~/lib/crypto/*": ["libs/crypto/src/*"],
      "~/lib/supabase/*": ["libs/db/src/*"],
      "~/env": ["libs/env/src/index.ts"],
      "~/lib/retell/*": ["libs/retell/src/*"],
      "~/server/*": ["apps/web/src/server/*"],
      "~/database.types": ["libs/types/src/database.types.ts"],
      "~/types/*": ["libs/types/src/*"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
