{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "bundler",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "module": "ESNext",
    "lib": ["es2022", "dom", "dom.iterable"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@odis/types": ["libs/types/src/index.ts"],
      "@odis/types/*": ["libs/types/src/*"],
      "@odis/validators": ["libs/validators/src/index.ts"],
      "@odis/validators/*": ["libs/validators/src/*"],
      "@odis/services": ["libs/services/src/index.ts"],
      "@odis/services/*": ["libs/services/src/*"],
      "@odis/utils": ["libs/utils/src/index.ts"],
      "@odis/utils/*": ["libs/utils/src/*"],
      "@odis/db": ["libs/db/src/index.ts"],
      "@odis/db/*": ["libs/db/src/*"],
      "@odis/ui": ["libs/ui/src/index.ts"],
      "@odis/ui/*": ["libs/ui/src/*"],
      "@odis/vapi": ["libs/vapi/src/index.ts"],
      "@odis/vapi/*": ["libs/vapi/src/*"],
      "@odis/idexx": ["libs/idexx/src/index.ts"],
      "@odis/idexx/*": ["libs/idexx/src/*"],
      "@odis/api-client": ["libs/api-client/src/index.ts"],
      "@odis/api-client/*": ["libs/api-client/src/*"],
      "@odis/email": ["libs/email/src/index.ts"],
      "~/lib/utils/*": ["libs/utils/src/*"],
      "~/lib/services/*": ["libs/services/src/*"],
      "~/lib/validators/*": ["libs/validators/src/*"],
      "~/lib/db/*": ["libs/db/src/*"],
      "~/lib/ui/*": ["libs/ui/src/*"],
      "~/lib/vapi/*": ["libs/vapi/src/*"],
      "~/lib/idexx/*": ["libs/idexx/src/*"],
      "~/lib/api-client/*": ["libs/api-client/src/*"],
      "~/lib/clinics/*": ["apps/web/src/lib/clinics/*"],
      "~/lib/ai/*": ["apps/web/src/lib/ai/*"],
      "~/lib/qstash/*": ["apps/web/src/lib/qstash/*"],
      "~/lib/resend/*": ["apps/web/src/lib/resend/*"],
      "~/lib/email": ["libs/email/src/index.tsx"],
      "~/lib/email/*": ["libs/email/src/*"],
      "~/lib/logger": ["apps/web/src/lib/logger/index.ts"],
      "~/lib/logger/*": ["apps/web/src/lib/logger/*"],
      "~/lib/api/*": ["apps/web/src/lib/api/*"],
      "~/lib/constants/*": ["apps/web/src/lib/constants/*"],
      "~/lib/llamaindex/*": ["apps/web/src/lib/llamaindex/*"],
      "~/lib/crypto/*": ["apps/web/src/lib/crypto/*"],
      "~/lib/supabase/*": ["libs/db/src/*"],
      "~/env": ["apps/web/src/env.js"],
      "~/lib/retell/*": ["apps/web/src/lib/retell/*"],
      "~/database.types": ["libs/types/src/database.types.ts"],
      "~/types/*": ["libs/types/src/*"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
