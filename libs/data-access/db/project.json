{
  "name": "data-access-db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-access/db/src",
  "projectType": "library",
  "tags": [
    "type:data-access",
    "scope:server",
    "platform:node"
  ],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "libs/data-access/db"
      },
      "cache": true,
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/libs/data-access/db/vitest.config.ts"
      ]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p tsconfig.json",
        "cwd": "libs/data-access/db"
      },
      "cache": true,
      "inputs": [
        "default",
        "^default"
      ]
    }
  }
}
