{
  "name": "domain-auth-util",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domain/auth/util/src",
  "projectType": "library",
  "tags": ["type:feature", "scope:server", "platform:node"],
  "targets": {
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "libs/domain/auth/util"
      },
      "cache": true,
      "inputs": ["default", "{workspaceRoot}/eslint.config.js"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p tsconfig.json",
        "cwd": "libs/domain/auth/util"
      },
      "cache": true,
      "inputs": ["default", "^default"]
    }
  }
}
