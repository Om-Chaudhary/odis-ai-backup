{
  "name": "domain-cases-data-access",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/domain/cases/data-access/src",
  "projectType": "library",
  "tags": ["type:service", "scope:server", "platform:node"],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "libs/domain/cases/data-access"
      },
      "cache": true,
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/libs/domain/cases/data-access/vitest.config.ts"
      ]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p tsconfig.json",
        "cwd": "libs/domain/cases/data-access"
      },
      "cache": true,
      "inputs": ["default", "^default"]
    }
  }
}
