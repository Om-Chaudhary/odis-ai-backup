{
  "name": "services-shared",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/services-shared/src",
  "projectType": "library",
  "tags": ["type:lib", "scope:services", "platform:node"],
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "libs/services-shared"
      },
      "cache": true,
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/libs/services-shared/vitest.config.ts"
      ]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p tsconfig.lib.json",
        "cwd": "libs/services-shared"
      },
      "cache": true,
      "inputs": ["default", "^default"]
    }
  }
}
