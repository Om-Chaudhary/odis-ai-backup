  "private": true,
  "type": "module",
  "scripts": {
    "build": "NX_NO_CLOUD=true NODE_ENV=production nx build web --verbose",
    "build:all": "NX_NO_CLOUD=true NODE_ENV=production nx run-many -t build",
    "dev": "NX_NO_CLOUD=true NODE_OPTIONS='--no-deprecation' nx dev web",
    "dev:pims-sync": "nx serve pims-sync",
    "dev:docs": "nx start docs",
    "start": "nx start web",
    "preview": "nx build web && nx start web",
    "kill-dev": "node scripts/kill-dev-servers.js",
    "lint": "nx lint web",
    "lint:all": "nx run-many -t lint",
    "lint:fix": "nx lint web --fix",
    "test:all": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx run-many -t test -- --run",
    "test:libs": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx run-many -t test --projects=tag:type:lib -- --run",
    "test:ui": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx test web --ui",
    "test:coverage": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx test web -- --coverage --run",
    "test:coverage:all": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx run-many -t test -- --coverage --run",
    "test:ci": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx run-many -t test -- --coverage --ci",
    "test:affected": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx affected -t test -- --run",
    "test:watch": "NX_NO_CLOUD=true SKIP_ENV_VALIDATION=true nx test web --watch",
    "typecheck": "nx typecheck web",
    "typecheck:all": "nx run-many -t typecheck",
    "check": "nx run-many -t lint,typecheck",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "update-types": "node scripts/update-types.js",
    "graph": "nx graph",
    "docs:nx": "node scripts/generate-nx-docs.js",
    "prepare": "husky",
    "docs:dev": "nx start docs",
    "docs:build": "nx build docs",
    "docs:serve": "nx serve docs",
    "docs:clear": "nx clear docs",
    "backfill:inbound-outcomes": "tsx apps/web/src/scripts/backfill-inbound-outcomes.ts"
